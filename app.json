{
  "expo": {
    "name": "monetize",
    "slug": "monetize",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.apps.hal.monetize",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.apps.hal.monetize"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "d6bda6c5-9ebf-48f9-9275-72d2bb6343f7"
      }
    },
    "plugins": [
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-5301151247209995~1714151435",
          "iosAppId": "ca-app-pub-5301151247209995~1419649721",
          "userTrackingUsageDescription": "関連性の高い広告を表示するためにIDをトラッキングいたします。",
          "delayAppMeasurementInit": true
        }
      ]
    ]
  }
}
